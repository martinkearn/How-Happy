@model HowHappy_Web.ViewModels.ResultViewModel

<h1>Test Result</h1>

<form asp-action="TestResult" enctype="multipart/form-data" id="upload-form">

    <h1><select asp-for="Emotion" asp-items="@Model.Emotions"></select></h1>

    <div id="upload-form-inputs">

        <div class="form-group" id="form-file-input">
            <div class="input-group">
                <span class="input-group-btn">
                    <span class="btn btn-default btn-file">
                        Browse <input type="file" name="file" id="file" class="form-control input-lg">
                    </span>
                </span>
                <input type="text" class="form-control" readonly>
            </div>
        </div>

        <div class="form-group" id="form-submit-button">
            <button type="submit" class="btn btn-primary btn-lg btn-block" id="submit-button" ><i class="fa fa-upload"></i> Upload</button>
        </div>

    </div>

</form>

@section Scripts{
    <script>
        var object = "@Model.ThemeColour";
        alert(object);
    </script>
}